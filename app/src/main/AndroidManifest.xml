<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sqlite.example.ContactsApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sqlite.example.ContactsApp.UpdateActivity"
            android:parentActivityName="com.sqlite.example.ContactsApp.MainActivity"/>
        <activity
            android:name="com.sqlite.example.ContactsApp.AddActivity"
            android:label="Add Contact"
            android:parentActivityName="com.sqlite.example.ContactsApp.MainActivity" />
        <activity
            android:name="com.sqlite.example.ContactsApp.MainActivity"
            android:label="Contacts">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>